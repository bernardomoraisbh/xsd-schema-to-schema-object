</xs:annotation>
</xs:element>
<xs:element name="maximumSections" minOccurs="0" maxOccurs="1" type="xs:integer" sawsdl:modelReference="http://iec.ch/TC57/CIM-generic#ShuntCompensator.maximumSections">
