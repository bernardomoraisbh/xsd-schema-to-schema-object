<xs:documentation>Rated voltage.</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="ShuntCompensatorControl" minOccurs="0" maxOccurs="1" sawsdl:modelReference="http://iec.ch/TC57/CIM-generic#ShuntCompensatorInfo.ShuntCompensatorControl">
<xs:annotation/>
